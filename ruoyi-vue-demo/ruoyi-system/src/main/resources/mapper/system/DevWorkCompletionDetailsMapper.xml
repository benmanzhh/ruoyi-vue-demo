<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.DevWorkCompletionDetailsMapper">
    
    <resultMap type="DevWorkCompletionDetails" id="DevWorkCompletionDetailsResult">
        <result property="id"    column="id"    />
        <result property="planId"    column="plan_id"    />
        <result property="completionDate"    column="completion_date"    />
        <result property="faultDescription"    column="fault_description"    />
        <result property="actionsTaken"    column="actions_taken"    />
        <result property="partsReplaced"    column="parts_replaced"    />
        <result property="reportUrl"    column="report_url"    />
        <result property="remark"    column="remark"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectDevWorkCompletionDetailsVo">
        select id, plan_id, completion_date, fault_description, actions_taken, parts_replaced, report_url, remark, create_by, create_time, update_by, update_time from dev_work_completion_details
    </sql>

    <select id="selectDevWorkCompletionDetailsList" parameterType="DevWorkCompletionDetails" resultMap="DevWorkCompletionDetailsResult">
        <include refid="selectDevWorkCompletionDetailsVo"/>
        <where>  
            <if test="planId != null "> and plan_id = #{planId}</if>
            <if test="completionDate != null "> and completion_date = #{completionDate}</if>
            <if test="faultDescription != null  and faultDescription != ''"> and fault_description = #{faultDescription}</if>
            <if test="actionsTaken != null  and actionsTaken != ''"> and actions_taken = #{actionsTaken}</if>
            <if test="partsReplaced != null  and partsReplaced != ''"> and parts_replaced = #{partsReplaced}</if>
            <if test="reportUrl != null  and reportUrl != ''"> and report_url = #{reportUrl}</if>
        </where>
    </select>
    
    <select id="selectDevWorkCompletionDetailsById" parameterType="Long" resultMap="DevWorkCompletionDetailsResult">
        <include refid="selectDevWorkCompletionDetailsVo"/>
        where id = #{id}
    </select>

    <insert id="insertDevWorkCompletionDetails" parameterType="DevWorkCompletionDetails" useGeneratedKeys="true" keyProperty="id">
        insert into dev_work_completion_details
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="planId != null">plan_id,</if>
            <if test="completionDate != null">completion_date,</if>
            <if test="faultDescription != null">fault_description,</if>
            <if test="actionsTaken != null">actions_taken,</if>
            <if test="partsReplaced != null">parts_replaced,</if>
            <if test="reportUrl != null">report_url,</if>
            <if test="remark != null">remark,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="planId != null">#{planId},</if>
            <if test="completionDate != null">#{completionDate},</if>
            <if test="faultDescription != null">#{faultDescription},</if>
            <if test="actionsTaken != null">#{actionsTaken},</if>
            <if test="partsReplaced != null">#{partsReplaced},</if>
            <if test="reportUrl != null">#{reportUrl},</if>
            <if test="remark != null">#{remark},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateDevWorkCompletionDetails" parameterType="DevWorkCompletionDetails">
        update dev_work_completion_details
        <trim prefix="SET" suffixOverrides=",">
            <if test="planId != null">plan_id = #{planId},</if>
            <if test="completionDate != null">completion_date = #{completionDate},</if>
            <if test="faultDescription != null">fault_description = #{faultDescription},</if>
            <if test="actionsTaken != null">actions_taken = #{actionsTaken},</if>
            <if test="partsReplaced != null">parts_replaced = #{partsReplaced},</if>
            <if test="reportUrl != null">report_url = #{reportUrl},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDevWorkCompletionDetailsById" parameterType="Long">
        delete from dev_work_completion_details where id = #{id}
    </delete>

    <delete id="deleteDevWorkCompletionDetailsByIds" parameterType="String">
        delete from dev_work_completion_details where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>